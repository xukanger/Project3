<!DOCTYPE html>
<html lang="en">
<head>
    <meta name="viewport" content="user-scalable=no, initial-scale=1, maximum-scale=1"/>
    <meta http-equiv="Content-Type" content="text/html;charset=utf-8" />
    <link rel="stylesheet" href="/style/css/lib/bootstrap-3.0.3/css/bootstrap.min.css"/>
    <script src="/style/js/build/lib/jquery/jquery-1.11.0.min.js"></script>
    <script src="/style/css/lib/bootstrap-3.0.3/js/bootstrap.min.js"></script>
    <link rel="stylesheet" href="/style/css/src/site/bops/bops.css$!{styleViewTools.version()}"/>
    <script src="/style/js/build/app/bootbox/bootbox-4.2.0.js"></script>
    <script src="/style/js/build/app/notify/notify.js"></script>
    <script src="/style/js/build/app/uploader/fileuploader.js"></script>
    <title>导入导出考生信息</title>
</head>
<body>
<h1>导入考生信息</h1>
<button id="upload" type="button" class="btn btn-primary">上传文件</button>
<br/>
<input id="excelPath" type="text" style="width:600px"/>
<script>
    jQuery(function(){
        excelUpload();
        function excelUpload(){
            var excelUploader = new qq.FileUploaderBasic({
                button: document.getElementById('upload'),
                action: '/bops/bopsUser/excelUpload.htm',
                allowedExtensions: ['xls','xlsx'],
                onComplete: function(id, fileName, res){
                    var btn = jQuery(this.button);
                    if(!res && !res.data){
                        btn.notify('上传出现异常，请稍候重试！', "error");
                        return;
                    }
                    if(res && res.result == 0){
                        $('#excelPath').val(res.data.path);
                    }else{
                        btn.notify(res.message, "error");
                    }
                },
                messages: {
                    typeError: "请上传xls,.xlsx格式的文件"
                },
                showMessage: function(msg){
                    btn.notify(res.message, "success");
                }
            });
        }
    })
</script>
</body>

</html>